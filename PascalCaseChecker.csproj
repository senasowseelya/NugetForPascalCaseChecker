<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>PascalCaseChecker</Title>
    <Description>Checks if Type Name is in Pascal Case or not and logs the output to console</Description>
    <PackageTags>pascal , casecheck, checkcase,case</PackageTags>
    <Version>$(VersionPrefix)1.3</Version>
    <AssemblyVersion>1.2.1.0</AssemblyVersion>
  </PropertyGroup>

</Project>
